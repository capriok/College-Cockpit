{
  "name": "college-cockpit",
  "author": "capriok",
  "repository": "https://github.com/capriok/College-Cockpit",
  "description": "copckpit for everytthing college",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.0.1",
    "@fullcalendar/interaction": "^5.0.0",
    "@fullcalendar/react": "^5.0.0",
    "@fullcalendar/timegrid": "^5.0.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "electron-is-dev": "^1.2.0",
    "godspeed": "^1.4.13",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-electron-web-view": "^2.0.1",
    "react-scripts": "3.4.1",
    "short-uuid": "^3.1.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "wait-on": "^5.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
